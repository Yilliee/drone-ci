From 98c7b3f8669add05e177f52a4162189cc1345325 Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?Yilli=C3=A9?= <yilliee@protonmail.com>
Date: Sun, 3 Apr 2022 12:52:30 +0500
Subject: [PATCH 2/3] Treewide : Rename to a51 temporarilly

---
 Android.mk                 | 2 +-
 AndroidProducts.mk         | 4 ++--
 BoardConfig.mk             | 4 ++--
 omni_m21.mk => omni_a51.mk | 8 ++++----
 4 files changed, 9 insertions(+), 9 deletions(-)
 rename omni_m21.mk => omni_a51.mk (82%)

diff --git a/Android.mk b/Android.mk
index 972f218..abc1222 100644
--- a/Android.mk
+++ b/Android.mk
@@ -12,7 +12,7 @@
 # See the License for the specific language governing permissions and
 # limitations under the License.
 
-ifneq ($(filter m21,$(TARGET_DEVICE)),)
+ifneq ($(filter a51,$(TARGET_DEVICE)),)
 
 LOCAL_PATH := $(call my-dir)
 
diff --git a/AndroidProducts.mk b/AndroidProducts.mk
index b73bbab..df434d5 100644
--- a/AndroidProducts.mk
+++ b/AndroidProducts.mk
@@ -14,6 +14,6 @@
 # limitations under the License.
 #
 
-PRODUCT_MAKEFILES := $(LOCAL_DIR)/omni_m21.mk
+PRODUCT_MAKEFILES := $(LOCAL_DIR)/omni_a51.mk
 
-COMMON_LUNCH_CHOICES := omni_m21-eng
+COMMON_LUNCH_CHOICES := omni_a51-eng
diff --git a/BoardConfig.mk b/BoardConfig.mk
index f175b54..4289e83 100644
--- a/BoardConfig.mk
+++ b/BoardConfig.mk
@@ -11,7 +11,7 @@
 # limitations under the License.
 #
 
-DEVICE_PATH := device/samsung/m21
+DEVICE_PATH := device/samsung/a51
 
 # Architecture
 TARGET_ARCH := arm64
@@ -109,7 +109,7 @@ BOARD_AVB_RECOVERY_ROLLBACK_INDEX := 1
 BOARD_AVB_RECOVERY_ROLLBACK_INDEX_LOCATION := 1
 
 # PBRP specific flags
-PB_DISABLE_DEFAULT_DM_VERITY := true
+#PB_DISABLE_DEFAULT_DM_VERITY := true
 PB_TORCH_PATH := "/sys/devices/virtual/camera/flash/rear_flash"
 PB_TORCH_MAX_BRIGHTNESS := 1
 
diff --git a/omni_m21.mk b/omni_a51.mk
similarity index 82%
rename from omni_m21.mk
rename to omni_a51.mk
index dedd77b..78aab96 100644
--- a/omni_m21.mk
+++ b/omni_a51.mk
@@ -5,14 +5,14 @@ $(call inherit-product, $(SRC_TARGET_DIR)/product/full_base_telephony.mk)
 $(call inherit-product, $(SRC_TARGET_DIR)/product/languages_full.mk)
 
 # Include any options that can't be included in BoardConfig.mk
-$(call inherit-product, device/samsung/m21/device.mk)
+$(call inherit-product, device/samsung/a51/device.mk)
 
 # Inherit some common PitchBlack stuff.
 $(call inherit-product, vendor/pb/config/common.mk)
 
 ## Device identifier. This must come after all inclusions
-PRODUCT_NAME := omni_m21
-PRODUCT_DEVICE := m21
-PRODUCT_MODEL := SM-M215F
+PRODUCT_NAME := omni_a51
+PRODUCT_DEVICE := a51
+PRODUCT_MODEL := SM-A515F
 PRODUCT_BRAND := samsung
 PRODUCT_MANUFACTURER := samsung
